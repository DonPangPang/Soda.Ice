@foreach (var tag in _tags)
{
    <BlogTag Item="@tag"></BlogTag>
}
@code {
    List<VBlogTag> _tags = new List<VBlogTag>()
    {
    };

    protected override Task OnInitializedAsync()
    {
        _tags = new List<VBlogTag>();
        for (var i = 0; i < 20; i++)
        {
            _tags.Add(new VBlogTag { Id = Guid.NewGuid(), Name = $"标签{i}" });
        }
        return base.OnInitializedAsync();
    }
}